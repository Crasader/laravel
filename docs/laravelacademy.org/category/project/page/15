return [
    &#039;driver&#039; =&gt; env(&#039;MAIL_DRIVER&#039;, &#039;smtp&#039;),
    &#039;host&#039; =&gt; env(&#039;MAIL_HOST&#039;, &#039;smtp.mailgun.org&#039;),
    &#039;port&#039; =&gt; e...</p>
